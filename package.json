{
  "name": "newforms",
  "description": "JavaScript port of Django's form-handling library, usable in browsers and Node.js, making use of React on both sides",
  "version": "0.5.0-dev",
  "author": "Jonathan Buchanan <jonathan.buchanan@gmail.com> (https://github.com/insin)",
  "tags": [
    "isomorphic",
    "forms",
    "validation",
    "type-coercion",
    "react"
  ],
  "main": "./lib/newforms",
  "repository": {
    "type": "git",
    "url": "http://github.com/insin/newforms.git"
  },
  "dependencies": {
    "Concur": "~0.2.4",
    "isomorph": "~0.2.1",
    "validators": "~0.3.0"
  },
  "devDependencies": {
    "browserify": "3.31.x",
    "browserify-shim": "^3.3.0",
    "gulp": "~3.5.5",
    "gulp-browserify": "0.4.x",
    "gulp-concat": "~2.1.7",
    "gulp-header": "~1.0.2",
    "gulp-jshint": "~1.3.4",
    "gulp-plumber": "~0.5.6",
    "gulp-rename": "1.1.x",
    "gulp-uglify": "~0.2.0",
    "gulp-util": "~2.2.13",
    "jshint-stylish": "~0.1.5",
    "qqunit": "0.6.x",
    "react": "0.9.0"
  },
  "scripts": {
    "test": "gulp build-js && node tests/tests.js",
    "dist": "gulp dist --production"
  },
  "browserify": {
    "transform": [ "browserify-shim" ]
  },
  "browserify-shim": {
    "react": "global:React"
  }
